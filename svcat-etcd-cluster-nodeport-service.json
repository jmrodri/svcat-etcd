{
    "kind": "Service",
    "apiVersion": "v1",
    "metadata": {
        "name": "svcat-etcd-cluster-client-service"
    },
    "spec": {
        "selector": {
            "etcd_cluster": "svcat-etcd-cluster",
            "app": "etcd"
        },
        "ports": [
            {
                "protocol": "TCP",
                "port": 2379,
                "targetPort": 2379,
                "nodePort": 32379
            }
        ],
        "type": "NodePort"
    }
}
